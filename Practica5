        "\n",
        "datos = [10,20,30,40,50]\n",
        "\n",
        "media = stats.mean(datos)\n",
        "\n",
        "mediana = stats.median(datos)\n",
        "\n",
        "modas = stats.multimode(datos)\n",
        "\n",
        "if len(modas) == len(datos):\n",
        "    modas = None\n",
        "\n",
        "print(\"Media:\", media)\n",
        "print(\"Mediana:\", mediana)\n",
        "print(\"Moda:\", modas)"
